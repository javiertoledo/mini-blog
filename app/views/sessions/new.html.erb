<h1>Log In</h1>
<%= form_for Admin.new, url: session_path do |f| -%>
  <p><%= alert %></p>

  <div>
    <%= f.label :email, "Email" %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :password, "Password" %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.submit "Log In", :class => 'btn btn-primary' %>
  </div>
<% end -%>
